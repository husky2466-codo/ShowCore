{
  "_meta": {
    "description": "Sample data for the Dashboard section including onboarding progress, stats, activity feed, and AI suggestions",
    "models": [
      {
        "name": "OnboardingProgress",
        "description": "Tracks user's onboarding completion status with tasks grouped by category",
        "fields": {
          "totalTasks": "Total number of onboarding tasks",
          "completedTasks": "Number of completed tasks",
          "requiredCompleted": "Number of required tasks completed",
          "requiredTotal": "Total number of required tasks",
          "percentComplete": "Completion percentage (0-100)",
          "canDismiss": "Whether checklist can be dismissed",
          "isDismissed": "Whether user has dismissed the checklist",
          "tasks": "Array of OnboardingTask items"
        }
      },
      {
        "name": "OnboardingTask",
        "description": "Individual onboarding task with status and action details",
        "fields": {
          "id": "Unique task identifier",
          "category": "Task category (profile/trust/payment/engagement)",
          "title": "Task title",
          "description": "Task description",
          "icon": "Icon identifier",
          "status": "Task status (pending/in-progress/completed/skipped)",
          "xpReward": "XP reward for technicians (optional)",
          "isRequired": "Whether task is required",
          "actionLabel": "Button label",
          "actionUrl": "Navigation URL",
          "estimatedMinutes": "Estimated time to complete",
          "order": "Display order"
        }
      },
      {
        "name": "DashboardStats",
        "description": "Role-specific dashboard metrics and statistics",
        "fields": {
          "role": "User role (technician or company)",
          "activeBookings": "Number of active bookings",
          "unreadMessages": "Number of unread messages",
          "currentTier": "Current tier (technicians only)",
          "xp": "Current experience points (technicians only)",
          "xpToNextTier": "XP needed for next tier (technicians only)",
          "xpProgress": "XP progress percentage (technicians only)",
          "pendingShowProofs": "Pending show proofs (technicians only)",
          "pendingReviews": "Pending reviews (companies only)",
          "techniciansHired": "Number of technicians hired (companies only)"
        }
      },
      {
        "name": "ActivityItem",
        "description": "Individual activity feed item showing recent platform events",
        "fields": {
          "id": "Unique activity identifier",
          "type": "Activity type (booking-request, message-received, etc.)",
          "title": "Activity title",
          "description": "Activity description",
          "icon": "Icon identifier",
          "iconColor": "Icon color (Tailwind class)",
          "timestamp": "ISO datetime",
          "relativeTime": "Human-readable time (e.g., '2 hours ago')",
          "actionUrl": "Optional navigation URL",
          "metadata": "Type-specific additional data"
        }
      },
      {
        "name": "AISuggestion",
        "description": "AI-powered recommendation card with action",
        "fields": {
          "id": "Unique suggestion identifier",
          "type": "Suggestion type",
          "title": "Suggestion title",
          "description": "Suggestion description",
          "icon": "Icon identifier",
          "priority": "Priority level (high/medium/low)",
          "actionLabel": "Button label",
          "actionUrl": "Navigation URL",
          "dismissible": "Whether suggestion can be dismissed",
          "estimatedImpact": "Expected impact description"
        }
      },
      {
        "name": "WelcomeBanner",
        "description": "Personalized welcome banner data",
        "fields": {
          "greeting": "Time-based greeting",
          "userName": "User's display name",
          "userAvatar": "Avatar image URL",
          "summaryMessage": "Summary status message",
          "timeOfDay": "Time of day (morning/afternoon/evening)"
        }
      },
      {
        "name": "AIConversation",
        "description": "AI Assistant conversation thread",
        "fields": {
          "id": "Unique conversation identifier",
          "messages": "Array of AIMessage items",
          "context": "Assistant context awareness data",
          "createdAt": "ISO datetime when conversation started",
          "lastMessageAt": "ISO datetime of last message"
        }
      },
      {
        "name": "AIMessage",
        "description": "Individual message in AI conversation",
        "fields": {
          "id": "Unique message identifier",
          "sender": "Message sender (user or assistant)",
          "content": "Message text content",
          "contentType": "Content type (text/action/suggestion)",
          "timestamp": "ISO datetime",
          "relativeTime": "Human-readable time",
          "actions": "Optional suggested actions",
          "isTyping": "Typing indicator flag"
        }
      }
    ]
  },
