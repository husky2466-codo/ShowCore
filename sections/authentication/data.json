{
  "_meta": {
    "description": "Sample data for the Authentication section",
    "models": [
      {
        "name": "User",
        "description": "Represents a user account with role-based profile data. Role determines whether this is a technician or company account.",
        "fields": {
          "id": "Unique identifier for the user",
          "email": "User's email address for authentication",
          "role": "Either 'technician' or 'company' - determines profile requirements and platform experience",
          "emailVerified": "Whether the user has verified their email address",
          "profileComplete": "Whether the user has completed role-specific profile setup",
          "createdAt": "ISO timestamp of account creation",
          "profile": "Role-specific profile data (TechnicianProfile or CompanyProfile)"
        }
      },
      {
        "name": "TechnicianProfile",
        "description": "Profile data specific to technician users",
        "fields": {
          "name": "Full name of the technician",
          "skills": "Array of skills/specializations",
          "location": "City and state/country",
          "hourlyRateRange": "Min/max hourly rate range",
          "bio": "Professional biography",
          "profilePhoto": "URL to profile photo (optional)",
          "portfolioLinks": "Array of portfolio URLs (optional)"
        }
      },
      {
        "name": "CompanyProfile",
        "description": "Profile data specific to company users",
        "fields": {
          "companyName": "Name of the company",
          "industry": "Primary industry/sector",
          "location": "City and state/country",
          "typicalHiringNeeds": "Description of typical technician requirements",
          "companyLogo": "URL to company logo (optional)",
          "website": "Company website URL (optional)"
        }
      },
      {
        "name": "OAuthProvider",
        "description": "Configuration for OAuth authentication providers",
        "fields": {
          "id": "Provider identifier (google, apple, microsoft)",
          "name": "Display name for the provider",
          "icon": "Icon identifier for the provider button",
          "enabled": "Whether this provider is currently available"
        }
      },
      {
        "name": "AuthError",
        "description": "Error messages for various authentication failures",
        "fields": {
          "code": "Error code identifier",
          "message": "User-facing error message",
          "field": "Form field associated with the error (optional)"
        }
      }
    ]
  },
